; Zona asir-drc.local
$TTL	86400
@	IN	SOA	dns.asir-drc.local. root.asir-drc.local. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			  86400 )	; Negative Cache TTL
;
@	    IN	NS	dns.asir-drc.local.
dns         IN	A	192.168.56.100
tierra      IN  A       192.168.56.101
discovery   IN  CNAME   tierra
