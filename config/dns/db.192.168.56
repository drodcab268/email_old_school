; BIND reverse data file for local loopback interface
$TTL	604800
@	IN	SOA	dns.asir-drc.local. root.asir-drc.local. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@		IN		NS	dns.asir-drc.local.
100		IN		PTR	dns.asir-drc.local.
101     IN      PTR asir-drc.local.
